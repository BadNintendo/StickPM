	<style>
main {
    width: max-content;
    position: relative;
    display: flex;
    height: calc(100vh - 100px);
    min-width: 100%;
}

.profiles-container {
    display: grid;
    grid-template-rows: 50% 50%;
    grid-auto-columns: calc(50vh - 72px / 2);
    grid-auto-flow: column;
}

.profile {
    width: 100%;
    overflow: hidden;
    background-color: #fff;
    position: relative;
}

.profile__thumbnail {
    position: relative;
	overflow: hidden;
}

.profile__thumbnail:before {
    content: "";
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.6);
}

.profile__thumbnail img {
    width: 100%;
    border-style: none;
}

.profile__role {
    font-weight: 700;
    text-transform: uppercase;
    color: #fc0;
}

.profile__role a,
.profile__meta a {
    color: inherit;
    transition: all .3s ease;
    text-decoration: none;
}

.profile__name {
    margin-bottom: 1.3vh;
    font-size: 4.1vh;
    color: #fff;
    text-transform: uppercase;
    font-family: Rajdhani, sans-serif;
    font-weight: 700;
    letter-spacing: -.06em;
}

.profile__meta {
    font-weight: 700;
    text-transform: uppercase;
    color: #fff;
}

.profile__body {
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    transition: transform .7s ease;
	padding: 32px 40px;
}

.profile:hover .profile__body {
    transform: translateY(0);
}

.profile__body {
    transform: translateY(193px);
}

.profile__excerpt {
    height: 120px;
    color: #c6cbea;
    transition: opacity .5s ease;
}
.main-nav .header-menu-toggle {
    position: absolute;
    top: 0;
    right: 45px;
}
.pagination-container {
  text-align: center;
  margin: 20px 0;
}

.pagination {
  list-style: none;
  display: inline-block;
  padding: 0;
  margin: 0;
}

.pagination li {
  display: inline;
}

.pagination li a,
.pagination li span.dots {
  color: #fff;
  background-color: #333;
  padding: 10px 15px;
  margin: 0 5px;
  text-decoration: none;
  font-size: 16px;
  font-weight: bold;
  border-radius: 5px;
  text-shadow: 5px 5px 4px black;
  transition: background-color 0.3s, color 0.3s;
}

.pagination li a:hover {
  background-color: #e67e00;
  color: #fff;
}

.pagination li a.active {
  background-color: #e67e00;
  color: #fff;
}

.pagination li .arrow {
  font-size: 18px;
  padding: 10px;
}

.pagination li .dots {
  color: #fff;
}

