		<main>
			<div class="profiles-container">
				<% profiles.forEach(profile => { %>
				<div class="profile <%= profile.isVip ? 'vip' : '' %>"> <!-- Add class vip for border coloring -->
					<div class="profile__thumbnail">
						<img src="<%= profile.profilePhoto %>" alt="<%= profile.username %>'s Avatar">
					</div>
					<div class="profile__body <%= profile.isFollowing ? 'Following' : '' %>"> <!-- Add class following for a star icon top right -->
						<div class="profile__header">
							<ul class="list-unstyled">
								<li class="profile__role">
									<a href="#">Role</a>
								</li>
							</ul>
							<h2 class="profile__name">
								<a href="/profile/<%= profile.username %>"><%= profile.username %></a>
							</h2>
							<ul class="list-unstyled">
								<li class="profile__meta">
									<a href="#"><%= profile.joinDate %></a>
								</li>
							</ul>
						</div>
						<div class="profile__excerpt">
							Followers: <%= profile.followingCount %><br />
							<%= profile.bio %>
						</div>
					</div>
				</div>
                <% }) %>
			</div>
		</main>
